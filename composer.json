{
    "name": "heystack/ecommerce-payment",
    "type": "silverstripe-module",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.heyday.net.nz/"
        }
    ],
    "require": {        
        "php": ">=5.4.0",
        "composer/installers": "~1.0.0"
    },
    "require-dev": {
        "silverstripe/framework": "~2.4",
        "heystack/heystack": "1.1.*@dev",
        "heystack/ecommerce-core": "1.1.*@dev",
        "symfony/class-loader": "~2.2",
        "phpmd/phpmd": "1.4.*",
        "pdepend/pdepend": "1.1.*",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Heystack": "code/"
        }
    },
    "extra": {
        "installer-paths": {
            "sapphire": [
                "silverstripe/framework"
            ]
        },
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}