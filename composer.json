{
    "name": "heystack/ecommerce-payment",
    "type": "silverstripe-module",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages:satis@packages.heyday.net.nz/"
        }
    ],
    "require": {        
        "php": ">=5.4.0",
        "composer/installers": "1.0.0"
    },
    "require-dev": {
        "silverstripe/framework": "2.4.9",
        "heystack/heystack": "dev-develop",
        "heystack/ecommerce-core": "dev-develop",
        "composer/composer": "1.0.0-alpha6",
        "phpmd/phpmd": "1.4.*",
        "phploc/phploc": "dev-master",
        "pdepend/pdepend": "1.1.*",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Heystack": "code/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev"
}